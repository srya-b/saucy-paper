	#z <- PS.env <- k rng #z_to_pw #pw_to_z #z_to_a #a_to_z ;
	sid = recv #z ;
	clist = recv #z ;
	
	$pw <- party_wrapper[K][K1][p2z,z2p][p2f,f2p][p2a,a2p] <- 
					k rng sid clist #pw_to_z #z_to_pw #pw_to_f #f_to_pw #pw_to_a #a_to_pw #z ;

